	"settings": {
		"custom": {
			"fluid": {
				"min-width": 320,
				"max-width": 1500,
				"min-size": 16,
				"max-size": 20,
				"min-ratio": 1.2,
				"max-ratio": 1.33,
				"screen": "100vw",
				"breakpoint": "calc((var(--wp--custom--fluid--screen) - ((var(--wp--custom--fluid--min-width) / 16) * 1rem)) / ((var(--wp--custom--fluid--max-width) / 16) - (var(--wp--custom--fluid--min-width) / 16)))",
				
				"min-scale-0": "var(--wp--custom--fluid--min-ratio)",
				"max-scale-0": "var(--wp--custom--fluid--max-ratio)",
				"min-size-0": "calc(var(--wp--custom--fluid--min-size) / 16)",
				"max-size-0": "calc(var(--wp--custom--fluid--max-size) / 16)",
				"size-0": "calc(((var(--wp--custom--fluid--min-size-0) * 1rem) + (var(--wp--custom--fluid--max-size-0) - var(--wp--custom--fluid--min-size-0)) * var(--wp--custom--fluid--breakpoint)))",
				
				"min-scale-1": "calc(var(--wp--custom--fluid--min-scale-0) * var(--wp--custom--fluid--min-ratio))",
				"max-scale-1": "calc(var(--wp--custom--fluid--max-scale-0) * var(--wp--custom--fluid--max-ratio))",
				"min-size-1": "calc(var(--wp--custom--fluid--min-scale-0) * var(--wp--custom--fluid--min-size) / 16)",
				"max-size-1": "calc(var(--wp--custom--fluid--max-scale-0) * var(--wp--custom--fluid--max-size) / 16)",
				"size-1": "calc(((var(--wp--custom--fluid--min-size-1) * 1rem) + (var(--wp--custom--fluid--max-size-1) - var(--wp--custom--fluid--min-size-1)) * var(--wp--custom--fluid--breakpoint)))",

				"min-scale-2": "calc(var(--wp--custom--fluid--min-scale-1) * var(--wp--custom--fluid--min-ratio))",
				"max-scale-2": "calc(var(--wp--custom--fluid--max-scale-1) * var(--wp--custom--fluid--max-ratio))",
				"min-size-2": "calc(var(--wp--custom--fluid--min-scale-1) * var(--wp--custom--fluid--min-size) / 16)",
				"max-size-2": "calc(var(--wp--custom--fluid--max-scale-1) * var(--wp--custom--fluid--max-size) / 16)",
				"size-2": "calc(((var(--wp--custom--fluid--min-size-2) * 1rem) + (var(--wp--custom--fluid--max-size-2) - var(--wp--custom--fluid--min-size-2)) * var(--wp--custom--fluid--breakpoint)))",

				"min-scale-3": "calc(var(--wp--custom--fluid--min-scale-2) * var(--wp--custom--fluid--min-ratio))",
				"max-scale-3": "calc(var(--wp--custom--fluid--max-scale-2) * var(--wp--custom--fluid--max-ratio))",
				"min-size-3": "calc(var(--wp--custom--fluid--min-scale-2) * var(--wp--custom--fluid--min-size) / 16)",
				"max-size-3": "calc(var(--wp--custom--fluid--max-scale-2) * var(--wp--custom--fluid--max-size) / 16)",
				"size-3": "calc(((var(--wp--custom--fluid--min-size-3) * 1rem) + (var(--wp--custom--fluid--max-size-3) - var(--wp--custom--fluid--min-size-3)) * var(--wp--custom--fluid--breakpoint)))",

				"min-scale-4": "calc(var(--wp--custom--fluid--min-scale-3) * var(--wp--custom--fluid--min-ratio))",
				"max-scale-4": "calc(var(--wp--custom--fluid--max-scale-3) * var(--wp--custom--fluid--max-ratio))",
				"min-size-4": "calc(var(--wp--custom--fluid--min-scale-3) * var(--wp--custom--fluid--min-size) / 16)",
				"max-size-4": "calc(var(--wp--custom--fluid--max-scale-3) * var(--wp--custom--fluid--max-size) / 16)",
				"size-4": "calc(((var(--wp--custom--fluid--min-size-4) * 1rem) + (var(--wp--custom--fluid--max-size-4) - var(--wp--custom--fluid--min-size-4)) * var(--wp--custom--fluid--breakpoint)))",

				"min-scale-5": "calc(var(--wp--custom--fluid--min-scale-4) * var(--wp--custom--fluid--min-ratio))",
				"max-scale-5": "calc(var(--wp--custom--fluid--max-scale-4) * var(--wp--custom--fluid--max-ratio))",
				"min-size-5": "calc(var(--wp--custom--fluid--min-scale-4) * var(--wp--custom--fluid--min-size) / 16)",
				"max-size-5": "calc(var(--wp--custom--fluid--max-scale-4) * var(--wp--custom--fluid--max-size) / 16)",
				"size-5": "calc(((var(--wp--custom--fluid--min-size-5) * 1rem) + (var(--wp--custom--fluid--max-size-5) - var(--wp--custom--fluid--min-size-5)) * var(--wp--custom--fluid--breakpoint)))"
			},
			"spacing": {
				"xs": "calc(var(--wp--custom--fluid--size-0) * 0.625)",
				"sm": "calc(var(--wp--custom--fluid--size-0) * 0.8)",
				"base": "var(--wp--custom--fluid--size-0)",
				"lg": "var(--wp--custom--fluid--size-1)",
				"xl": "var(--wp--custom--fluid--size-2)",
				"2-xl": "var(--wp--custom--fluid--size-3)",
				"3-xl": "var(--wp--custom--fluid--size-4)",
				"4-xl": "var(--wp--custom--fluid--size-5)",
				"full": "100vh"
			},
			"typography": {
				"font-size": {
					"xs": "calc(var(--wp--preset--fluid--size-0) * 0.625)",
					"sm": "calc(var(--wp--preset--fluid--size-0) * 0.8)",
					"base": "var(--wp--custom--fluid--size-0)",
					"lg": "var(--wp--custom--fluid--size-1)",
					"xl": "var(--wp--custom--fluid--size-2)",
					"2-xl": "var(--wp--custom--fluid--size-3)",
					"3-xl": "var(--wp--custom--fluid--size-4)",
					"4-xl": "var(--wp--custom--fluid--size-5)"
				}
			},
			"width": {
				"content": "calc(var(--wp--preset--font-size--base) * 40)",
				"wide": "calc(var(--wp--preset--font-size--base) * 60)"
			}
		},
		"appearanceTools": true,
		"color": {
